{
  "name": "ep-modpack",
  "version": "2.0.0",
  "description": "EP Modpack",
  "main": "src/main.js",
  "author": "Kurasava",
  "email": "flashbladepoint@mail.ru",
  "license": "ARR",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "devDependencies": {
    "@types/node": "^22.13.13",
    "electron": "^35.0.3",
    "electron-builder": "^25.1.8",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  },
  "build": {
    "appId": "ru.kurasava.modpack",
    "productName": "EP Modpack",
    "directories": {
      "output": "dist"
    },
    "files": [
      "**/*",
      "!node_modules/electron"
    ],
    "linux": {
      "target": "AppImage",
      "icon": "src/assets/img/icons"
    },
    "win": {
      "target": "portable",
      "icon": "src/assets/img/icon.ico"
    },
    "mac": {
      "target": "dmg",
      "icon": "src/assets/img/war-crimes.icns"
    }
  },
  "dependencies": {
    "prismarine-nbt": "^2.7.0"
  }
}
